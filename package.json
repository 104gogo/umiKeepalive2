{
  "name": "ssr-with-eggjs",
  "private": true,
  "scripts": {
    "dev": "egg-bin dev --port=7001 --sticky",
    "start": "egg-scripts start --port=7001 --daemon --title=egg-umi-ssr --env=prod",
    "stop": "egg-scripts stop --title=egg-umi-ssr",
    "build": "cross-env APP_ROOT=app/web umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@stomp/stompjs": "^5.4.4",
    "@stripe/react-stripe-js": "^1.1.1",
    "@stripe/stripe-js": "^1.3.0",
    "@umijs/hooks": "^1.9.2",
    "@umijs/preset-react": "^1.4.25",
    "ali-oss": "^6.6.0",
    "antd": "^4.3.5",
    "axios": "^0.19.2",
    "better-scroll": "^1.15.2",
    "cross-env": "^5.2.0",
    "draft-js": "^0.11.5",
    "egg": "^2.12.0",
    "egg-alinode": "^2.0.1",
    "egg-scripts": "^2.10.0",
    "egg-view-assets": "^1.3.0",
    "egg-view-nunjucks": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "js-audio-recorder": "^1.0.1",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.6.2",
    "qs": "^6.7.0",
    "rc-dialog": "^7.6.0",
    "rc-upload": "^2.9.4",
    "react-copy-to-clipboard": "^5.0.2",
    "react-cropper": "^1.3.0",
    "react-dom": "^16.8.6",
    "react-google-recaptcha-v3": "^1.5.1",
    "react-id-swiper": "^2.4.0",
    "react-infinite-scroller": "^1.2.4",
    "react-intl": "^4.5.5",
    "react-lazyload": "^2.6.5",
    "react-photo-view": "^0.3.9",
    "react-quill": "^1.3.3",
    "react-virtualized": "^9.21.2",
    "react-youtube": "^7.11.2",
    "swiper": "^5.3.0",
    "umi-request": "^1.2.19",
    "url": "^0.11.0",
    "whatwg-fetch": "^3.0.0",
    "zlib": "^1.0.5"
  },
  "peerDependencies": {
    "umi": "latest"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@types/qs": "^6.5.3",
    "@umijs/fabric": "^2.0.8",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-dva-hmr": "^0.4.0",
    "babel-plugin-import": "^1.12.0",
    "egg-bin": "^4.9.0",
    "egg-mock": "^3.20.1",
    "eslint": "^6.8.0",
    "http-proxy-middleware": "^1.0.4",
    "koa2-connect": "^1.0.2",
    "husky": "^4.0.7",
    "prettier": "^2.0.2",
    "lint-staged": "^10.0.8",
    "redbox-react": "^1.5.0",
    "shelljs": "^0.8.4",
    "umi": "^3.2.5",
    "webstorm-disable-index": "^1.2.0",
    "xlsx": "^0.15.6",
    "zip-local": "^0.3.4"
  }
}
